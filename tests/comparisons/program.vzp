let shouldBeTrue = && <@ true [
    8 > 4,
    8 >= 7,
    8 >= 8,
    8 == 8,
    8 <= 8,
    8 <= 9,
    8 < 9,
    8 != 10
]

let shouldBeFalse = || <@ false [
    3 > 4,
    4 > 4,
    3 >= 7,
    3 >= 8,
    3 == 8,
    9 <= 8,
    10 <= 9,
    9 < 9,
    10 < 9,
    10 != 10
]

if shouldBeTrue && !shouldBeFalse then print("good") else print("bad") 
