let Animal = type <|
    dog {
        trick :: [Int64]
    },
    cat {
        lives :: Int64
    }
|>

let f = (x :: Int64) =>
    if x > 0 then Animal.dog("fetch") else Animal.cat(100)


let d = f(4)

let g = (a :: Animal) =>
    switch a <|
        Animal.cat -> [that.lives],
        Animal.dog -> that.trick
    |>

print(g(d))
