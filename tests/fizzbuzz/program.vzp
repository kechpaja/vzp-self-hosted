let fizzbuzz = (x) =>
    if x < 0 then
        ""
    else 
        recur(x-1) + (if x % 15 == 0 then
            "fizzbuzz "
        else if x % 3 == 0 then
            "fizz "
        else if x % 5 == 0 then
            "buzz "
        else
            "number ") /* No toString enabled yet */

print(fizzbuzz(30))
